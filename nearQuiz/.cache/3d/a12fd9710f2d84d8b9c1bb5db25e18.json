rce":"components/QuestItem.js","original":{"line":73,"column":10}},{"generated":{"line":106,"column":15},"source":"components/QuestItem.js","original":{"line":22,"column":8}},{"generated":{"line":106,"column":16},"source":"components/QuestItem.js","original":{"line":21,"column":8}},{"generated":{"line":106,"column":17},"source":"components/QuestItem.js","original":{"line":20,"column":4}},{"generated":{"line":107,"column":0},"source":"components/QuestItem.js","original":{"line":79,"column":1}},{"generated":{"line":107,"column":1},"source":"components/QuestItem.js","original":{"line":3,"column":0}},{"name":"QuestItem","generated":{"line":109,"column":15},"source":"components/QuestItem.js","original":{"line":80,"column":15}}],"sources":{"components/QuestItem.js":"import React, { useState, useEffect } from 'react'\nimport {Button,Row,Col} from 'react-bootstrap';\nconst QuestItem = (props) => {\n    const [ quest, setQuest ] = useState(props.quest);\n    const [submitAns, setSubmitAns] = useState();\n    useEffect(\n    () => {\n      setQuest(props.quest)\n    },\n    [ props ]\n    )\n    const handleAnswerChange = event => {\n        setSubmitAns(parseInt(event.target.value))\n    }\n    const handleQuestSubmit = async() =>{\n        event.preventDefault();\n        const result = await window.contract.answerQuestion({_quest_id: quest.id,_choice: submitAns});\n        console.log(result)\n    }\n    return (\n        <div>\n        <form onSubmit={handleQuestSubmit}>\n        <Row>\n            <Col>\n            <label>\n              Question number:\n            </label>\n            </Col>\n            <Col>\n              <input type=\"text\" value={quest.id} readOnly={true} />\n            </Col>\n          </Row>\n          <Row>\n            <Col>\n            <label>\n              Question detail:\n            </label>\n            </Col>\n            <Col>\n              <input type=\"text\" value={quest.question} readOnly={true} />\n            </Col>\n          </Row>\n          <Row>\n            <Col>\n            <label>\n              Answer 1:\n            </label>\n            </Col>\n            <Col>\n              <input type=\"text\" value={quest.choice1} readOnly={true}/>\n            </Col>\n          </Row>\n          <Row>\n            <Col>\n            <label>\n              Answer 2:\n            </label>\n            </Col>\n            <Col>\n              <input type=\"text\" value={quest.choice2} readOnly={true} />\n            </Col>\n          </Row>\n          <Row>\n            <Col>\n            <label>\n              YOur Answer :\n            </label>\n            </Col>\n            <Col>\n              <input type=\"text\" onChange={handleAnswerChange} />\n            </Col>\n          </Row>\n          <Row>\n            <Button variant=\"success\" onClick={handleQuestSubmit}>Submit</Button>\n          </Row>\n          </form>\n        </div>\n    )\n}\nexport default QuestItem\n"},"lineCount":null}},"error":null,"hash":"e0b5b42993447b8089b449828f470a28","cacheData":{"env":{}}}